<script>

var hash =  document.location.hash + '';
if (hash > '')
{
    var f = document.forms;
    if (f)
    {
        var j = f.length;
        for (var i = 0; i < j; i++)
        {
            if (f[i].url)
              f[i].url.value = f[i].url.value + hash;
        }
    }
}

</script>

